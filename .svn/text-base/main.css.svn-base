.bg {background:#EAF3FD;padding:2px; max-width:430px}
.grey {color:#999;}
#onlyme {font-weight:normal}
#checkall + label {font-size:15px; color:#0066CC}
#checkall + label:hover{color:#CC0000}
.btn {
	display:inline-block;
	position:relative;
	-moz-border-radius:3px 3px 3px 3px; /*for FF*/
	border-radius: 3px 3px; /*For chrome*/
	background:-moz-linear-gradient(center top , #F9F9F9, #E3E3E3);/*for FF*/
	background: -webkit-gradient(linear, 0% 40%, 0% 70%, from(#F9F9F9), to(#E3E3E3));/*For chrome*/
	border:1px solid #BBBBBB;
	border-top:1px solid #CCCCCC;
	border-bottom:1px solid #A0A0A0;
	cursor:default;
	font:menu;
	font-size:12px;
	outline:none;
	text-align:center;
	vertical-align:middle;
	white-space:nowrap;
	-webkit-user-select: none;
	-moz-user-select: none;
	/*text-shadow:0px 1px 0px #FFF;*/
	/*-webkit-box-shadow: 0px 1px 1px #FFF;
	-moz-box-shadow: 0px 1px 0px #FFF;*/
}
div.btn:active, label.btn:active {
	background: -webkit-gradient(linear, 0% 40%, 0% 70%, from(#E3E3E3), to(#F9F9F9));/*For chrome*/
	background:-moz-linear-gradient(center top, #E3E3E3 , #F9F9F9) repeat scroll 0 0 transparent;/*for FF*/
	cursor:default
}
div.btn:hover {
	border-color:#939393;
}
.color {
	width:16px;
	height:16px;
	background:#FEEB85;
	vertical-align:top
}
.arrow{
	width:8px;
	height:9px;
	margin:0 5px 2px 3px;
	vertical-align: middle;	
}
#fmn{
	display:none;
	position:absolute;
	z-index:400;
	background:#FFF;
	border-left:solid 1px #CCCCCC;
	border-top:solid 1px #CCCCCC;
	border-right:solid 1px #999;
	border-bottom:solid 1px #999;
	padding:4px;
	text-shadow:none;
	text-align:left;
	-moz-box-shadow: 1px 1px 1px #EBEBEB ;
	-webkit-box-shadow: 1px 1px 1px #EBEBEB;
	left:243px;
 	-moz-border-radius:3px 3px 3px 3px; /*for FF*/
	border-radius: 3px 3px; /*For chrome*/
	background:-moz-linear-gradient(center top , #F9F9F9, #E3E3E3);/*for FF*/
	background: -webkit-gradient(linear, 0% 40%, 0% 70%, from(#F9F9F9), to(#E3E3E3));/*For chrome*/
	width:145px
}
